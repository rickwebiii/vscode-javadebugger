{
	"compilerOptions": {
		"target": "ES5",
		"module": "commonjs",
		"inlineSourceMap": false,
		"sourceMap": true,
		"outDir": "../out",
		"noImplicitAny": false,
		"removeComments": false,
		"preserveConstEnums": true,
		"strictNullChecks": true,
		"noUnusedParameters": false,
		"noUnusedLocals": true
	}
}

